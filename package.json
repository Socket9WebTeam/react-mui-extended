{
  "name": "@socket9/react-mui-extended",
  "version": "1.0.0",
  "description": "Seven Peaks react components library",
  "main": "lib/index.node.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:staged-files": "npm run test -- --bail --findRelatedTests --passWithNoTests",
    "coverage": "npm run test -- --coverage --ci --silent",
    "type-check": "tsc -p tsconfig.json",
    "lint": "eslint src",
    "generate": "plop",
    "build:types": "tsc -p .",
    "build:rollup": "rollup -c",
    "build": "npm run build:types && npm run build:rollup",
    "prepare": "husky install"
  },
  "files": [
    "/lib/*",
    "CHANGELOG.md"
  ],
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "npm run test:staged-files"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "rollup": "^2.79.1",
    "rollup-plugin-import-css": "^3.3.1",
    "typescript": "^5.1.5"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
